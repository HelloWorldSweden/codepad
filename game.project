[project]
title = Codepad
version = 1.1
bundle_resources = /codepad/bundle_resources/, instructions
bundle_exclude_resources = 

[bootstrap]
main_collection = /main/main.collectionc
render = /codepad/rendering/custom.renderc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[library]
include_dirs = codepad

[display]
width = 720
height = 720

[physics]
scale = 0.02

[script]
shared_state = 1

[html5]
htmlfile = /codepad/template.html

